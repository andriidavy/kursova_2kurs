<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="employeeMainPage"
            type="com.example.registration.fragment.employeeFragments.EmployeeMainPageFragment" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayoutEmployeeMainPage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragment.employeeFragments.EmployeeMainPageFragment">

        <Button
            android:id="@+id/button_to_custom_in_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/custom_in_progress_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_margin="30dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>